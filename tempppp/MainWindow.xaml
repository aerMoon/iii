<Window x:Class="tempppp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tempppp"
        mc:Ignorable="d"
        Title="Autorization" MaxHeight="300" MinHeight="300"
        MinWidth="300" MaxWidth="300">
    <StackPanel VerticalAlignment="Center">
        <TextBlock HorizontalAlignment="Center">login</TextBlock>
        <TextBox x:Name="loginTB" Width="200" TextAlignment="Center"></TextBox>
        <TextBlock HorizontalAlignment="Center" Margin="0,20,0,0" Cursor="">password</TextBlock>
        <TextBox x:Name="passTB" Width="200" TextAlignment="Center"></TextBox>
        <Button Content="enter" Click="Button_Click" Width="200" Margin="0,10,0,0"/>
    </StackPanel>
</Window>
 